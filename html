        let visitorCount = Math.floor(Math.random() * 9000000) + 1000000;
        let currentUsers = Math.floor(Math.random() * 100) + 20;
        
        setInterval(() => {
            visitorCount += Math.floor(Math.random() * 50);
            currentUsers = Math.floor(Math.random() * 100) + 20;
            document.getElementById('visitor').textContent = visitorCount.toLocaleString();
            document.getElementById('current').textContent = currentUsers;
        }, 3000);
        
